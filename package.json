        "Fajar Muhammad Hamka (https://github.com/fajartd02)"
    ],
    "license": "MIT",
    "scripts": {
        "auto": "yarn compile && yarn start",
        "start": "node ./dist/server.js",
        "compile": "yarn clean && tsc",
        "compile:debug": "yarn compile --diagnostics",
        "clean": "rimraf ./dist",
        "typeorm": "node ./node_modules/typeorm/cli.js",
        "lint": "eslint ./src",
        "lint:fix": "eslint ./src --fix",
        "jwt:generate": "node ./scripts/jwt-generate.js",
        "seed": "yarn compile && node ./dist/seeder.js"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/luxon": "^2.3.1",
        "@types/node": "^17.0.23",
        "@types/pg": "^8.6.5",
        "@typescript-eslint/eslint-plugin": "^5.18.0",
        "@typescript-eslint/parser": "^5.18.0",
        "eslint": "^8.12.0",
        "eslint-import-resolver-typescript": "^2.7.1",
        "eslint-plugin-import": "^2.25.4",
        "rimraf": "^3.0.2",
        "typescript": "^4.6.3"
    },
    "dependencies": {
        "bcrypt": "^5.0.1",
        "cors": "^2.8.5",
        "dotenv": "^16.0.0",
        "express": "^4.17.3",
        "helmet": "^5.0.2",
        "http-status-codes": "^2.2.0",
        "joi": "^17.6.0",
        "jsonwebtoken": "^8.5.1",
        "luxon": "^2.3.1",
        "pg": "^8.7.3",
        "reflect-metadata": "^0.1.13",
        "typeorm": "^0.3.4"
    }
}
